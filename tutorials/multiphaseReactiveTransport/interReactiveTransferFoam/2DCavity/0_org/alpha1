FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1.0; 

boundaryField
{
    right
    {
        type            zeroGradient;
    }
    left
    {
        type            fixedValue;
        value           uniform 1;
    }
    top
    {
        type            zeroGradient;
    }
    bottom
    {
        type            constantAlphaContactAngle;
        theta0          60;
        limit           zeroGradient;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
    }
}


