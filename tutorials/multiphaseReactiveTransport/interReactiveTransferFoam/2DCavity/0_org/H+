FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      H+;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 1 0 0];

internalField   uniform 9.97413e-08; 

boundaryField
{
    right
    {
        type            zeroGradient;
    }
    left
    {
        type            fixedValue;
        value           uniform 9.97413e-08;
    }
    top
    {
        type            zeroGradient;
    }
    bottom
    {
        type            globalConcentrationMixed;
    }
    frontAndBack
    {
        type            empty;
    }
}


